# pl.pg (chạy trong thư mục logs/)
datafile = "log_20251206_124120.csv"
set datafile separator ","
set term pngcairo size 1200,700 font "Arial,12"
set output "log_20251206_124120.png"
set grid
set key outside

plot \
  datafile using 1:2  with lines lw 3 lc rgb "#D35400"  title "roll", \
  datafile using 1:3  with lines lw 2 dt 2 lc rgb "yellow"   title "Kalman roll", \
  datafile using 1:4 with lines lw 3 lc rgb "blue"          title "uM"
